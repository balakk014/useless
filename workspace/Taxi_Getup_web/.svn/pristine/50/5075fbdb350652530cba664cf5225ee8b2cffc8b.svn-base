/* Login Register Start */
#sign_up {
        -moz-border-radius: 6px;
        background: #fefefe;
        -webkit-border-radius: 6px;
         border-radius: 6px;
        border: 1px solid #536376;
        -webkit-box-shadow: rgba(0,0,0,.6) 0px 2px 12px;
        -moz-box-shadow:  rgba(0,0,0,.6) 0px 2px 12px;;
        padding: 14px 22px;
        width: 60%;
        position: relative;
        display: none;       
    }

#dispatch_popup {
        -moz-border-radius: 6px;
        background: #fefefe;
        -webkit-border-radius: 6px;
         border-radius: 6px;
        border: 1px solid #536376;
        -webkit-box-shadow: rgba(0,0,0,.6) 0px 2px 12px;
        -moz-box-shadow:  rgba(0,0,0,.6) 0px 2px 12px;;
        padding: 14px 22px;
        width: 60%;
        position: relative;
        display: none;       
    }

#driverlist_popup {
        -moz-border-radius: 6px;
        background: #fefefe;
        -webkit-border-radius: 6px;
         border-radius: 6px;
        border: 1px solid #536376;
        -webkit-box-shadow: rgba(0,0,0,.6) 0px 2px 12px;
